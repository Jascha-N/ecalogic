#! /bin/sh

# ecalogic launcher script
#
# Environment:
# JAVACMD        - Java command to use (optional)
# JAVA_OPTS      - JVM options (optional)

ECALOGIC_HOME="${0%/*}/.."

"${JAVACMD:-java}" $JAVA_OPTS -Decalogic.home="$ECALOGIC_HOME" -cp "$ECALOGIC_HOME/lib/ecalogic.jar" nl.ru.cs.ecalogic.ECALogic "$@"
